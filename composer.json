{
    "name": "omnipay/omnipay",
    "type": "library",
    "description": "Includes Omnipay payment processing library and factories for Http Client and ServerRequest",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "purchase"
    ],
    "homepage": "https://github.com/thephpleague/omnipay",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Macneil",
            "email": "adrian@adrianmacneil.com"
        },
        {
            "name": "Barry vd. Heuvel",
            "email": "barryvdh@gmail.com"
        },
        {
            "name": "Omnipay Community",
            "homepage": "https://github.com/thephpleague/omnipay/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "^6",
        "omnipay/common": "^3@dev",
        "zendframework/zend-diactoros": "^1.1"
    },
    "require-dev": {
        "omnipay/tests": "^3@dev"
    },
    "autoload": {
        "psr-4": { "League\\Omnipay\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "League\\Omnipay\\" : "tests/" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
